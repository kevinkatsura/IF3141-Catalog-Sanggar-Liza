<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record model="ir.ui.view" id="catalog_costume_tree">
    <field name="name">catalog.constume.tree</field>
    <field name="model">catalog.costume</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="description"/>
        <field name="stock"/>
        <field name="image"/>
      </tree>
    </field>
  </record>
  
</odoo>

